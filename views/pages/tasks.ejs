<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Todos</title>
	</head>
	<body>
		<ul>
			<% todosArray.forEach(function(task) { %>
			<li>
				<strong><%= task.title %> </strong>| Completed: <%= task.completed ===
				false ? 'No' : 'Yes' %>
			</li>
			<% }); %>
		</ul>
	</body>
</html>
